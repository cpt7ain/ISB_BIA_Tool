<Window x:Class="ISB_BIA_IMPORT1.Helpers.MsgWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISB_BIA_IMPORT1.Helpers"
        mc:Ignorable="d"
        Title="" 
        Height="Auto" 
        Width="Auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Image x:Name="MsgImg"/>
            <TextBlock x:Name="MsgBlock" Text="{Binding Msg_Text}"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="Ok"/>
            <Button x:Name="Cancel"/>
            <Button x:Name="Ja"/>
            <Button x:Name="Nein"/>
        </StackPanel>
    </StackPanel>
</Window>
